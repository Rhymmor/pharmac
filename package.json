{
  "name": "pharmac",
  "version": "1.0.0",
  "description": "Pharmac",
  "license": "private",
  "private": true,
  "scripts": {
    "start-db": "./scripts/start-db.sh",
    "dev-webpack": "node scripts/dev-server.js",
    "dev-backend": "nodemon --watch 'src/**/*.ts' --ignore src/client --exec 'ts-node' src/server/main.ts",
    "dev": "concurrently -p name -c bgWhite,bgBlue,bgMagenta,bgWhite --kill-others -n DB,API,WEB,PY 'npm run start-db' 'npm run dev-backend' 'npm run dev-webpack' 'npm run solver'",
    "backend": "ts-node src/server/main.ts",
    "frontend": "node scripts/prod/prod-server.js",
    "solver": "python3 src/solver-server/main.py",
    "compile": "sh ./scripts/compile-prod.sh",
    "start": "concurrently -p name --kill-others -n API,WEB,PY 'npm run backend' 'npm run frontend' 'npm run solver'"
  },
